{
  "compilerOptions": {
    "target": "ES2019",
    "module": "ES6",
    "moduleResolution": "node",
    "outDir": "static/js",
    "rootDir": "static/ts",
    "strict": true,
    "jsx": "react-jsx",
    "types": ["node", "react"],
    "esModuleInterop": true,
    "baseUrl": "./",
    "paths": {
      "@/*": ["static/ts/*"]
    }
  },
  "include": ["static/ts/**/*"]
}
